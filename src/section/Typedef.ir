::Typedef <%.Str
  context\!context.Context\$
  ^(context)

:output\A.<output.Line>\
  decoder := &type.Decoder(@.context.pkg, @.strlines().join(''))
  out := &output.Line(@.lines[0])
  out.indent = 0
  out.lines.appendLines([#])
    docLines(['@typedef {' + decoder.output() + '}'])
    @.context.name.decl + ';'
  => [out]
