::Typedef <%.Str
  context\!context.Context\$
  %(context)

:output\A.<output.Line>\
  decoder := new TypeDecoder(@.context.pkg, @.strlines().join(''))
  out := new output.Line(@.lines[0])
  out.indent = 0
  out.lines.append_lines([#])
    doc_lines(['@typedef {' + decoder.output() + '}'])
    @.context.name.decl() + ';'
  => [out]
