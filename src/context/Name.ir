Name in file scope.
::
  @pkg+\~Package\$
  @id+\s\$

:decl+\s\
  => (@pkg.empty() ? 'var ' : '') + @pkg.fullname(@id)

:ref+\s\
  => @pkg.fullname(@id)

:global\~\
  => &~(&~.Package(''), @id)

:property\~\
  id\s\$
  => &~(&~.Package(@.ref + '.prototype'), id)

:staticProperty\~\
  id\s\$
  => &~(&~.Package(@.ref), id)

:toString\s\
  => '[' + @pkg + ':' + @id + ']'
