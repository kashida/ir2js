Name in file scope.
::
  @pkg+\%.Package\$
  @id+\s\$

:decl+\s\
  => @pkg.fullname(@id)

:ref+\s\
  => @pkg.fullname(@id)

:global\%.Name\
  => &%.Name(&%.Package(COMPILED_PKGS_BASE), @id)

:property\%.Name\
  id\s\$
  => &%.Name(&%.Package(@.ref + '.prototype'), id)

:staticProperty\%.Name\
  id\s\$
  => &%.Name(&%.Package(@.ref), id)

:toString\s\
  => '[' + @pkg + ':' + @id + ']'
