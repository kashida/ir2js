Name in file scope.

::Name
  @pkg+\!%.Package\$
  @id+\s\$

:decl+\s\
  => (@pkg.empty() ? 'var ' : '') + @pkg.fullname(@id)

:ref+\s\
  => @pkg.fullname(@id)

:global\!%.Name\
  => &%.Name(&%.Package(''), @id)

:property\!%.Name\
  id\s\$
  => &%.Name(&%.Package(@.ref + '.prototype'), id)

:staticProperty\!%.Name\
  id\s\$
  => &%.Name(&%.Package(@.ref), id)

:toString\s\
  => '[' + @pkg + ':' + @id + ']'
