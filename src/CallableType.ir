::CallableType
  @name\s\$
  @parent\s\ ''
  @methods\A.<CallableType>\ []
  @args\A.<s|null>\ []

TODO: use setter.
:setParent
  parent_name\s\$
  @parent = parent_name

:addMethod
  name\s\$
  m := &CallableType(name)
  @methods.push(m)
  => m

:addArg
  arg\s|null\$
  @args.push(arg)

:extract\O\
  obj := {name: @name, args: @args}
  if @parent
    obj['parent'] = @parent
  if @methods
    obj['methods'] = @methods.map(##)
      m\CallableType\$
      => m.extract()
  => obj
