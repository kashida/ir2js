:=warn ##
  line\input.Line\$
  msg\s\? '*warning*'
  console.warn(msg + ' (line ' + line.line_no + '): ' + line.line)

:=assert ##
  check\*\$
  line\input.Line\? UnknownInputLine
  msg\s\? '*assertion*'
  console.assert(#)
    check
    msg + (line ? ' (line ' + line.line_no + '): ' + line.line : '')
