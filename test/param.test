#{ multi-line init
::classname
  param\n\? a + b
  | + c + d
  @member\n\? func(#)
    a
    b
    c
#=
/**
 * @param {number=} opt_param
 * @param {number=} opt_member
 * @constructor
 * @struct
 */
var classname = function(opt_param, opt_member) {
  var self = this;
  var param = opt_param === undefined ? (a + b + c + d) : opt_param;
  /**
   * @type {number}
   * @private
   */
  this._member = opt_member === undefined ? (func(
    a,
    b,
    c
  )) : opt_member;
};
classname.prototype._classname = 'classname';
{
  cls:[
    {
      name:classname
      args:[
        Number
        Number
      ]
    }
  ]
}

#}

#{ re with param format in it
::Class
:method
  if /^\s*\/\//.test(@str)
    =>
#=
/**
 * @constructor
 * @struct
 */
var Class = function() {
var self = this;
};
Class.prototype._classname = 'Class';
Class.prototype.method = function() {
  var self = this;
  if (/^\s*\/\//.test(self._str)) {
    return;
  }
};
{
  cls:[
    {
      name:Class
      methods:[
        {
          name:method
        }
      ]
    }
  ]
