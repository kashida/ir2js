#{ comment lines
::classname
  // first comment.
  param\b\$
  // second comment.
  @member\b\? true
  // last comment.
#=
/**
 * @param {boolean} param
 * @param {boolean=} opt_member
 * @constructor
 * @struct
 */
var classname = function(param, opt_member) {
  var self = this;
  // first comment.
  // second comment.
  /**
   * @type {boolean}
   * @private
   */
  this._member = opt_member === undefined ? (true) : opt_member;
  // last comment.
};
classname.prototype._classname = 'classname';
{
  cls:[
    {
      name:classname
      args:[
        Boolean
        Boolean
      ]
    }
  ]
}

#}

