#{ class scope vars.
:::abc = 3
  // this comment should not make a block.
this should be a global comment.
:::def = [#]
  0
  ghi
  # + #
    xyz
    xxx
#=
var abc = 3;
  // this comment should not make a block.
/*
this should be a global comment.
*/
var def = [
  0,
  ghi,
  (
    xyz
  ) + (
    xxx
  )
];
{
  "cls": [],
  "fns": []
}
#}

#{ scope levels in global
::Klass
:abc = 0
::def = 1
:::ghi = 2
#=
/**
 * @constructor
 * @struct
 * @suppress {checkStructDictInheritance}
 */
var Klass = function() {
var self = this;
};
Klass.prototype._classname = 'Klass';
Klass.abc = 0;
var def = 1;
var ghi = 2;
{
  "cls": [
    {
      "name": "Klass",
      "args": [],
      "methods": []
    }
  ],
  "fns": []
}
#}

#{[zzz] scope levels in a pacakge
::Klass
:abc = 0
::def = 1
:::ghi = 2
#=
/**
 * @constructor
 * @struct
 * @suppress {checkStructDictInheritance}
 */
$zzz.Klass = function() {
var self = this;
};
$zzz.Klass.prototype._classname = '$zzz.Klass';
$zzz.Klass.abc = 0;
$zzz.def = 1;
var ghi = 2;
{
  "cls": [
    {
      "name": "$zzz.Klass",
      "args": [],
      "methods": []
    }
  ],
  "fns": []
}
#}
