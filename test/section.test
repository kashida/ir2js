#{ scope levels in global
::Klass
:abc = 0
::def = 1
:::ghi = 2
#=
/**
 * @constructor
 * @struct
 * @suppress {checkStructDictInheritance}
 */
_o_.Klass = function() {
var self = this;
};
_o_.Klass.prototype._classname = '_o_.Klass';
_o_.Klass.abc = 0;
_o_.def = 1;
_o_.ghi = 2;
{
  "cls": [
    {
      "name": "_o_.Klass",
      "args": [],
      "methods": []
    }
  ],
  "fns": []
}
#}

